<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <%- include('./css'); %>
    <!-- <script src="https://kit.fontawesome.com/0c783909db.js" crossorigin="anonymous"></script> -->
    <%- typeof isGCSSFile !== 'undefined' ? isGCSSFile : ''; %>
    <%- typeof isPCSSFile !== 'undefined' ? isPCSSFile : ''; %>
    <%- typeof isPageTitle !== 'undefined' ? '<title>' + getFirstUpperCase(isPageTitle) + '</title>' : ''; %>
    <%- startTagName([ { name : 'link', param : { type : 'image/x-icon', href : 'favicon.ico', }, }, ]); %>
</head>
<body>
    <% sidebarValue =  3; contentValue = (12 - sidebarValue); %>
    <%- startTagName([ { name : 'div', param : { id : 'container', }, }, ]); %>
        <%- startTagName([ { name : 'div', param : { class : [ 'row', ], }, }, ]); %>
        <% if (!sidebarValue) { } else { %> 
            <%- startTagName([ { name : 'div', param : { class : [ ('col-' + sidebarValue), ], }, }, ]); %>
            <%- include('./sidebar'); %>
            <%- endTagName([ { name : 'div', }, ]); %>
        <% } %>
            <%- startTagName([ { name : 'div', param : { class : [ ('col-' + contentValue), ], }, }, ]); %>
                <%- include('./navbar'); %>